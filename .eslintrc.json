{
//    "extends": "airbnb-base",
  "ecmaFeatures": {
    "modules": true,
    "spread" : true,
    "restParams" : true
  },
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    },
  "parserOptions": {
    "sourceType": "module"
  }
  }
